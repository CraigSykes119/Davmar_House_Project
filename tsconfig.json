{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "jsx": "preserve",
    "module": "ESNext",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  },
  "include": [".astro/types.d.ts", "src/**/*"],
  "exclude": ["node_modules", "dist"]
}
